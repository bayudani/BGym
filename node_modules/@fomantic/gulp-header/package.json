{
  "name": "@fomantic/gulp-header",
  "version": "2.1.1",
  "description": "Gulp extension to add header to file(s) in the pipeline.",
  "main": "./index.js",
  "proxy": null,
  "https-proxy": null,
  "directories": {
    "test": "test"
  },
  "jshintConfig": {
    "esversion": 8,
    "strict": "implied",
    "undef": true,
    "unused": true,
    "mocha": true,
    "node": true
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fomantic/gulp-header.git"
  },
  "keywords": [
    "header",
    "gulpplugin",
    "eventstream"
  ],
  "author": {
    "name": "Michael J. Ryan",
    "email": "tracker1@gmail.com",
    "url": "http://github.com/tracker1"
  },
  "contributors": [
    {
      "name": "GoDaddy.com",
      "url": "http://github.com/godaddy"
    },
    {
      "name": "Douglas Duteil",
      "email": "douglasduteil@gmail.com",
      "url": "http://github.com/douglasduteil"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gulp-community/gulp-header/issues"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "parser": "babel",
    "jsxBracketSameLine": true
  },
  "dependencies": {
    "concat-with-sourcemaps": "^1.1.0",
    "map-stream": "^0.1.0",
    "through2": "^3.0.2"
  },
  "devDependencies": {
    "gulp": "^5.0.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "expect": "^27.5.1",
    "should": "^13.2.3",
    "vinyl": "^3.0.0"
  },
  "scripts": {
    "test": "mocha ",
    "coverage": "nyc mocha",
    "publish-major": "npm version major && git push origin master && git push --tags",
    "publish-minor": "npm version minor && git push origin master && git push --tags",
    "publish-patch": "npm version patch && git push origin master && git push --tags"
  }
}
